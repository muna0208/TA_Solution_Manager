<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.daumsoft.taSolutionManager.restFullApi.mapper.PreprocessorRestMapper">

   <select id="getPreprocessorList" resultType="preProcessor">
		SELECT 
		* 
		FROM TB_PREPROCESSOR
   </select>

	<select id="getPreprocessor" resultType="preProcessor">
		SELECT 
		* 
		FROM TB_PREPROCESSOR 
		WHERE PREPROCESSOR_SEQ = #{preprocessorSeq}
   </select>

</mapper>